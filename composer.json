{
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.76",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.5"
    },
    "scripts": {
      "lint": "vendor/bin/php-cs-fixer fix --dry-run --diff",
      "lint:fix": "php-cs-fixer fix",
      "analyse": "phpstan analyse",
      "test": "vendor/bin/phpunit"
    },
    "autoload": {
        "psr-4": {
          "App\\": "src/"
        }
      },
      "autoload-dev": {
        "psr-4": {
          "App\\Tests\\": "tests/"
        }
      }


}
